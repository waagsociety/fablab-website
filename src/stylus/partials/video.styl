.videoWrapper 
    position relative
    width 100% !important
    padding-bottom 56.25% !important /* 169 */
    height 0

.videoWrapper iframe 
    position absolute
    top 0
    left 0
    width 100%
    height 100%


#video-viewport 
    position absolute
    top 0
    overflow hidden

#debug 
    top 0
    position absolute
    z-index 100
    color #fff

.covervid-wrapper 
    position absolute
    top 0
    width 100%
    height 100%
    overflow hidden
.covervid-wrapper video
    min-height 100%
    min-width 100%
